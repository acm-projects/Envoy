{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef": "off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-console": "off",
        "prefer-destructuring": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off"
    }
}